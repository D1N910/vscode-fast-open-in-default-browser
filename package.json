{
    "name": "fast-open-html-in-browser",
    "displayName": "Fast Open HTML in Default Browser",
    "description": "Can Use Keybindings Open HTML in Default Browser",
    "version": "0.0.4",
    "publisher": "d1n910",
    "repository": {
        "type": "git",
        "url": "https://github.com/d1n910/vscode-open-in-default-browser.git"
    },
    "bugs": {
        "url": "https://github.com/d1n910/vscode-open-in-default-browser/issues"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "keybindings",
        "open",
        "html",
        "default",
        "browser"
    ],
    "icon": "icon.jpg",
    "activationEvents": [
        "onCommand:D1n910.openInBrowser"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "D1n910.openInBrowser",
                "title": "Open in default browser"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorLangId == html",
                    "command": "D1n910.openInBrowser",
                    "group": "1_modification"
                }
            ],
            "explorer/context": [
                {
                    "when": "resourceLangId == html",
                    "command": "D1n910.openInBrowser",
                    "group": "1_modification"
                }
            ]
        },
        "keybindings": [
            {
                "command": "D1n910.openInBrowser",
                "when": "resourceLangId == html",
                "key": "ctrl+1",
                "mac": "cmd+1"
            }
        ]
    },
    "scripts": {
        "pub": "vsce publish patch"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "opn": "^4.0.2"
    }
}